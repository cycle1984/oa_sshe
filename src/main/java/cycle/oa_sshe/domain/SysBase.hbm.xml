<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="cycle.oa_sshe.domain.SysBase" table="SysBase">
        <id name="id" type="java.lang.Integer">
            <column name="ID" />
            <generator class="native" />
        </id>
        <property name="defPath" type="java.lang.String"  length="100">
            <column name="defPath" length="100"/>
        </property>
        <property name="refleshTime" type="java.lang.Integer" length="2">
            <column name="refleshTime" length="2"/>
        </property>
    </class>
</hibernate-mapping>
